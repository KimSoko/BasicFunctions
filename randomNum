function randomNum(min, max) {
  // returns integers only; to return floats remove the Math.floor() portion
  return Math.floor(Math.random() * (max - min) + min);
}

console.log(randomNum(1, 6));
// because I used Math.floor(), you have to set the max at 1 + (the actual max you want to see), because otherwise it will always round down and never get there. In this instance, 6 will never appear

